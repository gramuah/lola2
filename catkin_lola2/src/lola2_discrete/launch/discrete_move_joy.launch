<launch>
	<node name="f710_joy_node" pkg="joy" type="joy_node">
        <param name="autorepeat_rate" type="double" value="6"/>
 	</node>

	<rosparam file="$(find lola2_discrete)/config/discrete_move.yaml" command="load"/>
	<node name="discrete_move_joy" pkg="lola2_discrete" type="discrete_move_joy.py" output="screen"/>

</launch>
